{
    "name": "silmaralberti/php-cache",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "keywords": [
        "cache",
        "redis",
        "igbinay"
    ],
    "authors": [
        {
            "name": "alberti",
            "email": "silmaralberti@yahoo.com",
            "homepage": "https://github.com/silmar-alberti"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "ext-redis": "*",
        "php": "^7.2"
    },
    "suggest": {
        "ext-igbinary": "low memory and cache space utilization"
    },
    "autoload": {
        "psr-4": {
            "PhpCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "Tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.2.0"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.x"
    }
}