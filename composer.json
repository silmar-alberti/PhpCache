{
    "name": "silmaralberti/php-cache",
    "type": "library",
    "license": "MIT",
    "version": "1.1.2",
    "keywords": [
        "cache",
        "redis",
        "igbinay"
    ],
    "authors": [
        {
            "name": "alberti",
            "email": "silmaralberti@yahoo.com",
            "homepage": "https://github.com/silmar-alberti"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "ext-redis": "*",
        "php": "^7.2|^8.0"
    },
    "suggest": {
        "ext-igbinary": "low memory and cache space utilization"
    },
    "autoload": {
        "psr-4": {
            "PhpCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "Tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    }
}
